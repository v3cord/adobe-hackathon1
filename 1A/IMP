docker build --progress=plain -t pdf-extractor:latest .
docker run --rm -v %cd%/input:/app/input -v %cd%/output:/app/output --network none pdf-extractor:latest

docker build --platform linux/amd64 --progress=plain -t pdf-extractor:latest .




CLEAR DOCKER 

docker container prune -f
docker image prune -a -f
docker volume prune -f
docker network prune -f
docker system prune -a --volumes -f
